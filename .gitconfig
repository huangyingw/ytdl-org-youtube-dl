[remote "origin"]
    url = git@github.com:ytdl-org/youtube-dl.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/ytdl-org-youtube-dl.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/tools/ytdl-org-youtube-dl
